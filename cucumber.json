{
    "default":{
        "paths":["src/test/features"],
        "formatOptions":{
            "snippetInterface":"async-await"
        },
        "require":[
            "src/test/steps/*.js",
            "src/hooks/hooks.js"
        ],
        "tags":"",
        "monochrome":true,
        "dryRun":false,
        "format":[
            "html:test-results/report/cucumber-report.html",
            "json:test-results/report/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel":1,
        "retry":0
    },
    "rerun":{
        "formatOptions":{
            "snippetInterface":"async-await"
        },
        "require":[
            "src/test/steps/*.js",
            "src/hooks/hooks.js"
        ],
        "tags":"",
        "monochrome":true,
        "dryRun":false,
        "format":[
            "html:test-results/report/cucumber-report.html",
            "json:test-results/report/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel":1
    }
}
