{
  "name": "playwright_automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear-app": "node excel-converter/clearTestFiles.js",
    "convert-all": "node excel-converter/autoTestTool.js all",
    "convert-dmp": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/DMP/DMPTestSteps.xlsx 1",
    "convert-grants": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/Grants/GrantsTestSteps.xlsx 6",
    "convert-search": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/Search/SearchTestSteps.xlsx 6",
    "convert-cxe": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/CXE/CXETestSteps.xlsx 6",
    "convert-irad-admin": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/IRAD/IRADAdminTestSteps.xlsx 1",
    "convert-irad-input": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/IRAD/IradInputTestSteps.xlsx 1",
    "convert-irad-search": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/IRAD/IradSearchTestSteps.xlsx 1",
    "convert-irad-test": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/IRAD_TEST/IRADAdminTestSteps_NU.xlsx 1",
    "convert-search-test": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/TestFiles/SearchTestSteps_1.xlsx 6",
    "convert-test": "node excel-converter/autoTestTool.js excel-converter/ExcelFiles/IRAD_TEST/IRADSearchTestSteps_1.xlsx 1",
    "format-dmp":"node excel_formatter/excelFormatTool.js excel-converter/ExcelFiles/DMP/DMPTestSteps.xlsx 1",
    "format-irad-admin":"node excel_formatter/excelFormatTool.js excel-converter/ExcelFiles/IRAD/IRADAdminTestSteps.xlsx 1",
    "clean:reports": "rimraf test-results && mkdirp test-results",
    "generate:report": "node src/helper/reportGenerate.js && node ./PostTestScripts/updateCucumberReport.js",
    "pretest": "npm run clean:reports",
    "posttest": "npm run generate:report",
    "execute:script": "cucumber-js",
    "test": "npm run execute:script || npm run posttest",
    "execute:script:failed": "cucumber-js -p rerun @rerun.txt",
    "test:failed": "npm run execute:script:failed || npm run generate:report",
    "setup-pw": "npm install && npx playwright install",
    "cucumber-test": "cucumber-js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.11.0"
  },
  "dependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "node-fetch": "^2.7.0",
    "open": "^8.4.2",
    "uuid": "^9.0.1"
  }
}
